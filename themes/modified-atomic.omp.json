// 20220310020034
// https://raw.githubusercontent.com/Robiussani152/oh-my-posh/main/themes/atomic.omp.json

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "leading_diamond": "╭─",
          "properties": {
            "template": " {{ .Name }} "
          },
          "style": "diamond",
          "type": "shell"
        },
        {
          "background": "#ef5350",
          "foreground": "#FFFB38",
          "properties": {
            "template": "<parentBackground></>  "
          },
          "style": "diamond",
          "type": "root"
        },
        {
          "background": "#FF9248",
          "foreground": "#2d3436",
          "powerline_symbol": "",
          "properties": {
            "folder_icon": "  ",
            "home_icon": "",
            "style": "folder",
            "template": " {{ .Path }} "
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#FFFB38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#C792EA{{ end }}"
          ],
          "foreground": "#011627",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350>  {{ .Staging.String }}</>{{ end }} "
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "style": "roundrock",
            "template": " 羽{{ .FormattedMs }}⠀",
            "threshold": 0
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#303030",
          "foreground": "#3C873A",
          "leading_diamond": "",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a></> ",
            "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
            "yarn_icon": " <#348cba></>"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "node"
        },
        {
          "background": "#306998",
          "foreground": "#FFE873",
          "leading_diamond": "",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "python"
        },
        {
          "background": "#0e8ac8",
          "foreground": "#ec2729",
          "leading_diamond": "",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "java"
        },
        {
          "background": "#0e0e0e",
          "foreground": "#0d6da8",
          "leading_diamond": "",
          "properties": {
            "template": " {{ if .Unsupported }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "dotnet"
        },
        {
          "background": "#ffffff",
          "foreground": "#06aad5",
          "leading_diamond": "",
          "properties": {
            "template": "ﳑ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "go"
        },
        {
          "background": "#f3f0ec",
          "foreground": "#925837",
          "leading_diamond": "",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "rust"
        },
        {
          "background": "#e1e8e9",
          "foreground": "#055b9c",
          "leading_diamond": " ",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "dart"
        },
        {
          "background": "#ffffff",
          "foreground": "#ce092f",
          "leading_diamond": " ",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "angular"
        },
        {
          "background": "#945bb3",
          "foreground": "#359a25",
          "leading_diamond": " ",
          "properties": {
            "template": "<#ca3c34></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "julia"
        },
        {
          "background": "#ffffff",
          "foreground": "#9c1006",
          "leading_diamond": "",
          "properties": {
            "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "ruby"
        },
        {
          "background": "#ffffff",
          "foreground": "#5398c2",
          "leading_diamond": "",
          "properties": {
            "template": "<#f5bf45></> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "azfunc"
        },
        {
          "background": "#565656",
          "foreground": "#faa029",
          "leading_diamond": "",
          "properties": {
            "template": " {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
          },
          "style": "diamond",
          "trailing_diamond": " ",
          "type": "aws"
        },
        {
          "background": "#316ce4",
          "foreground": "#ffffff",
          "leading_diamond": "",
          "properties": {
            "template": "ﴱ {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "kubectl"
        },
        {
          "background": "#b2bec3",
          "foreground": "#222222",
          "leading_diamond": "",
          "properties": {
            "linux": "",
            "macos": "",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}<#262626> </>",
            "windows": ""
          },
          "style": "diamond",
          "type": "os"
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#b8e994{{end}}",
            "{{if eq \"Discharging\" .State.String}}#fff34e{{end}}",
            "{{if eq \"Full\" .State.String}}#33DD2D{{end}}"
          ],
          "foreground": "#262626",
          "invert_powerline": true,
          "leading_diamond": "",
          "properties": {
            "charged_icon": " ",
            "charging_icon": " ",
            "discharging_icon": " ",
            "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} <#262626></>"
          },
          "style": "diamond",
          "type": "battery"
        },
        {
          "background": "#40c4ff",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "leading_diamond": "",
          "properties": {
            "template": "  {{ .CurrentDate | date .Format }} ",
            "time_format": "_2,15:04"
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#21c7c7",
          "properties": {
            "template": "╰─"
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#e0f8ff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "template": "ﬀ "
          },
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "osc99": true,
  "version": 1
}
